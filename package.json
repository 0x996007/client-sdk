{
  "name": "client-sdk",
  "version": "0.1.1",
  "description": "client SDK fro ordering system",
  "type": "module",
  "engines": {
    "node": ">= 20 < 23"
  },
  "main": "build/src/index.js",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.10.5",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/eslint-plugin": "^1.1.24",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-api-utils": "^2.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run unit --config __tests__/vitest.config.ts",
    "test:coverage": "vitest run unit --config __tests__/vitest.config.ts --coverage.enabled --coverage.all",
    "prettier": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --check",
    "test:watch": "vitest unit"
  },
  "author": "0x996007<0x996007@users.noreply.github.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@cosmjs/amino": "^0.32.4",
    "@cosmjs/encoding": "^0.32.4",
    "@cosmjs/math": "^0.32.4",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@cosmjs/tendermint-rpc": "^0.32.4",
    "@cosmjs/utils": "^0.32.4",
    "@dydxprotocol/v4-proto": "^8.0.0",
    "@osmonauts/lcd": "^1.0.3",
    "@scure/bip32": "^1.6.1",
    "@scure/bip39": "^1.5.1",
    "axios": "^1.7.9",
    "bech32": "^2.0.0",
    "bech32-buffer": "^0.2.1",
    "bignumber.js": "^9.1.2",
    "cosmjs-types": "^0.9.0",
    "ethereum-cryptography": "^3.0.0",
    "ethers": "^6.13.5",
    "lodash": "^4.17.21",
    "long": "^5.2.3",
    "protobufjs": "^7.4.0",
    "tslib": "^2.8.1",
    "ws": "^8.18.0"
  },
  "volta": {
    "node": "22.12.0"
  }
}
